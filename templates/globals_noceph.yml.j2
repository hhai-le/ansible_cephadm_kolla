---
kolla_base_distro: "ubuntu"
kolla_internal_vip_address: "{{ virtual_ip }}"
network_interface: "{{ NIC_mgmt }}"
neutron_external_interface: "{{ NIC_external }}"
storage_interface: "{{ NIC_storage }}"
swift_storage_interface: "{{ '{{' }} storage_interface {{ '}}' }}"
tunnel_interface: "{{ NIC_tunnel }}"
neutron_plugin_agent: "openvswitch"
enable_cinder: "yes"
enable_fluentd: "yes"
enable_neutron_dvr: "yes"
enable_neutron_agent_ha: "no"
enable_neutron_provider_networks: "yes"
enable_nova_ssh: "yes"
nova_compute_virt_type: "kvm"
#enable_prometheus: "yes"
enable_central_logging: "yes"
#neutron_ovn_distributed_fip: "no"